@media screen and (prefers-reduced-motion: no-preference) {
  input.FormControl-checkbox[type="checkbox"]:checked::before { animation: 80ms cubic-bezier(0.65, 0, 0.35, 1) 80ms 1 normal forwards running checkmarkIn; }
}

